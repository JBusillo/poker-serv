{
	"presets": [
		[
			"env",
			{
				"targets": { "node": "12.0" },
				"useBuiltIns": true
			}
		]
	],
	"plugins": [
		// "transform-object-rest-spread",
		[
			"module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"test": "./test",
					"underscore": "lodash"
				}
			}
		]
	],
	"sourceMaps": true,
	"inputSourceMap": true
}
