{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"comma-dangle": "off",
		"lines-around-comment": "off",
		"no-console": "off",
		"brace-style": "off",
		"react/jsx-no-bind": "off",
		"no-unused-vars": "warn",
		"new-cap": "off"
	},
	"extends": "eslint-config-synacor"
}
